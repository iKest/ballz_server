{
    "extends": ["airbnb-standard", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error"],
        "no-param-reassign": ["error", {
            "props": false
        }]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "commonjs": true
    }
}
